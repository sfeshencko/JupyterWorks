    "def lag(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    if n == 1:\n",
    "        return 0\n",
    "    else:\n",
    "        return (1/(n+1))*(2*n)*lag(n)-(n/(n+1))*lag(n-1)\n",
    "print lag(2)"
