   "source": [
    "import re\n",
    "f = open('sentences.txt','r')\n",
    "lines = f.readline()\n",
    "for lines in f:\n",
    "    words = re.split('[^a-z]',lines.lower())\n",
    "    d = filter(None, words)\n",
    "print d"
   ]
